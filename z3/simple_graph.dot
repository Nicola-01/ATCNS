digraph simple_graph {
    node1 [label="x = input()"];
    node2 [label="y = input()"];
    node3 [label="if x + y > 10 goto node4 else goto node5"];
    node4 [label="z = x * 2"];
    node5 [label="z = y * 3"];
    node6 [label="if z == 15 goto node7 else goto node8"];
    node7 [label="result = 'valid'"];
    node8 [label="result = 'invalid'"];

    node1 -> node2;
    node2 -> node3;
    node3 -> node4;
    node3 -> node5;
    node4 -> node6;
    node5 -> node6;
    node6 -> node7;
    node6 -> node8;
}
